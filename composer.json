                "Shooorov\\Generator\\GeneratorServiceProvider"
            ],
            "aliases": {
                "Generator": "Shooorov\\Generator\\Facades\\Generator"
            }
        }
    },
    "minimum-stability": "dev",
    "require": {
        "php": "^8.0",
        "illuminate/contracts": "^9.0|^10.0",
