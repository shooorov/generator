        "generator",
        "laravel",
        "vue",
		"builder",
		"quick builder"
    ],
    "homepage": "https://github.com/shooorov/generator",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Shooorov\\Generator\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Shourov Shahadat",
            "email": "shooorov@gmail.com"
        },
        {
            "name": "Arif Hasan",
            "email": "arif.saiket@gmail.com"
        }
    ],
    "extra": {
		"branch-alias": {
            "dev-master": "1.x-dev"
        },
